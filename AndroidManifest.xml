<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lzh.dmremote"
    android:versionCode="24"
    android:versionName="1.7.5" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name_show" >


        <activity
            android:name="com.lzh.dmcontroler.MainTabActivity"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name_show" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="com.lzh.dmcontroler.ChannelListAct"></activity>        
        <activity android:name="com.lzh.dmcontroler.CurrentChnActivity"></activity>
        <activity android:name="com.lzh.dmcontroler.EmptyActivity"></activity>
        
    </application>

</manifest>
